@charset "UTF-8";

@font-face {
  font-family: "leavesfont";
  src:url("../fonts/leavesfont.eot");
  src:url("../fonts/leavesfont.eot?#iefix") format("embedded-opentype"),
    url("../fonts/leavesfont.woff") format("woff"),
    url("../fonts/leavesfont.ttf") format("truetype"),
    url("../fonts/leavesfont.svg#leavesfont") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "leavesfont" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="lf-"]:before,
[class*=" lf-"]:before {
  font-family: "leavesfont" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.lf-autoreload:before {
  content: "a";
}
.lf-deploy:before {
  content: "b";
}
.lf-preprocess:before {
  content: "d";
}
.lf-server:before {
  content: "f";
}
.lf-i18n:before {
  content: "e";
}
.lf-extensible:before {
  content: "c";
}
.lf-globe:before {
  content: "g";
}
.lf-love:before {
  content: "h";
}
